@using Microsoft.AspNet.Identity

@{
    ViewBag.Title = "Home: " + @User.Identity.GetUserName();
    Layout = "~/Views/Shared/_LayoutStudent.cshtml";
}

<div class="container float-left">
    <h2 class="mp-page-title">User: @User.Identity.GetUserName()</h2>
    <hr />
</div>

<div class="container float-left">
    <p class="text-primary">Your user account has no personal profile associated. Please see IT support to attach your profile to see personalised information here.</p>
</div>

